# Delete from the end of the array

def removeEnd(arr, length):
    if length > 0:
        arr[length - 1] = 0  # Overwrite the last element with 0

# Example array
arr = [4, 5, 6]

# Length of the array
length = len(arr)

# Call the function to remove the last element
removeEnd(arr, length)

# The array will now be [4, 5, 0]
print(arr)
